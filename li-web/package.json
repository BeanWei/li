{
  "name": "li-web",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier -c --write \"**/*.{ts,tsx,md}\" --end-of-line auto"
  },
  "devDependencies": {
    "@arco-design/web-react": "^2.32.2",
    "prettier": "^2.6.2",
    "turbo": "^1.2.6"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=14.0.0"
  },
  "packageManager": "pnpm@6.32.1"
}
